                                                        </TableCell>
                                                        <TableCell>
                                                            <Paragraph>
                                                                <Border BorderBrush="Black" BorderThickness="0,0,0, 1" Margin="0">
                                                                    <TextBlock>Enclose separate statement for :</TextBlock>
                                                                </Border>
                                                            </Paragraph>
                                                        </TableCell>
                                                    </TableRow>
                                                </TableRowGroup>
                                            </Table>
                                        </TableCell>
                                    </TableRow>
                                    <TableRow>
                                        <TableCell>
                                            <Table Margin="0">
                                                <Table.Columns>
                                                    <TableColumn Width="40"></TableColumn>
                                                    <TableColumn Width="30"></TableColumn>
                                                    <TableColumn></TableColumn>
                                                </Table.Columns>
                                                <TableRowGroup>
                                                    <TableRow>
                                                        <TableCell></TableCell>
                                                        <TableCell>
                                                            <Paragraph>(a)</Paragraph>
                                                        </TableCell>
                                                        <TableCell>
                                                            <Paragraph>Any income of the spouse of the assessee (if she/he  is not an assessee), minor children and dependant.</Paragraph>
                                                        </TableCell>
                                                    </TableRow>
                                                    <TableRow>
                                                        <TableCell></TableCell>
                                                        <TableCell>
                                                            <Paragraph>(b)</Paragraph>
                                                        </TableCell>
                                                        <TableCell>
                                                            <Paragraph>Tax exempted/tax free income.</Paragraph>
                                                        </TableCell>
                                                    </TableRow>


                                                </TableRowGroup>
                                            </Table>
                                        </TableCell>
                                    </TableRow>
                                    <TableRow>
                                        <TableCell>
                                            <Table Margin="0, 5, 0, 0">
