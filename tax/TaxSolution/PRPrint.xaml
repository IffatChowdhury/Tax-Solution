                                                        <TableCell>
                                                            <Paragraph>(6)</Paragraph>
                                                        </TableCell>
                                                        <TableCell>
                                                            <Paragraph>The assesse shall submit his/her photograph with return after every five year.</Paragraph>
                                                        </TableCell>
                                                    </TableRow>
                                                </TableRowGroup>
                                            </Table>
                                        </TableCell>
                                    </TableRow>
