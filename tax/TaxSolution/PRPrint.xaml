                                                    <TableRow>
                                                        <TableCell>
                                                            <Paragraph>(5)</Paragraph>
                                                        </TableCell>
                                                        <TableCell>
                                                            <Paragraph>Documents furnished to support the declaration should be signed by the assessee or his/her authorized representative.</Paragraph>
                                                        </TableCell>
                                                    </TableRow>
                                                </TableRowGroup>
                                            </Table>
                                        </TableCell>
